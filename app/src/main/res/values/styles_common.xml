<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WrapMatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="WrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="MatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="MatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="WrapMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="WrapMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="WrapWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="WrapWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="MatchMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="MatchMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="MatchWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="MatchWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchOne">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="MatchHeight_34">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_34</item>
    </style>
    <style name="MatchHeight_55">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_55</item>
    </style>
    <style name="MatchHeight_55.Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_55</item>
        <item name="android:orientation">horizontal</item>
    </style>



    <!--待考虑-->
    <style name="MatchHeight_30">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_30</item>
    </style>
    <style name="MatchHeight_33">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_33</item>
    </style>
    <style name="MatchHeight_44">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_44</item>
    </style>

    <!-- line-->
    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/line_e3e3e3</item>
    </style>
    <style name="line_white">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">@color/white_ffffff</item>
    </style>
    <style name="line_gray_eeeeee">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2px</item>
        <item name="android:background">@color/gray_eeeeee</item>
    </style>

    <!--dialog-->
    <style name="TimePickerDialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>

</resources>
